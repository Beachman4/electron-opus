{
  "name": "electron-opus",
  "description": "Electron native binding to OPUS",
  "version": "0.1.11",
  "author": "Justin Chase <justin@evolvelabs.com>",
  "homepage": "https://github.com/EvolveLabs/electron-opus",
  "bugs": "https://github.com/EvolveLabs/electron-opus/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/EvolveLabs/electron-opus.git"
  },
  "main": "index.js",
  "scripts": {
    "install": "bash ./node_modules/.bin/elinst"
  },
  "//": "Platform limitations is due to lack of config.h files. See README.md for more information.",
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm",
    "ia86"
  ],
  "engines": {
    "node": ">=2.2.1"
